def id : (a : Type) -> a -> a =
    fn a => fn x => x;


def const : (a : id Type Type) -> (b : Type) -> a -> b -> a =
    fn a => fn b => fn x => fn y => x;


def constagain : (a : Type) -> (b : Type) -> a -> b -> a =
    fn a => fn b => fn x => fn y => id a x;


def constswitch : (a : Type) -> a -> (b : Type) -> b -> a =
    fn a => fn x => fn b => fn y => x;


def add : Int32 -> Int32 -> Int32 =
    fn x => fn y => x;


def Nat : Type =
    Int32;


def n : Int32 =
    4;
